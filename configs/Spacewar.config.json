      "binaryEnv": [
        "bin"
      ]
    },
    {
      "name": "gcc-arm64",
      "url": "https://github.com/mvaisakh/gcc-build/releases/download/11012026/eva-gcc-arm64-11012026.tar.gz",
      "binaryEnv": [
        "bin"
      ]
    },
    {
      "name": "gcc-arm32",
      "url": "https://github.com/mvaisakh/gcc-build/releases/download/11012026/eva-gcc-arm-11012026.tar.gz",
      "binaryEnv": [
        "bin"
