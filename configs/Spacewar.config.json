  "enableCcache": true,
  "enableKernelSU": false,
  "enableLXC": false,
  "enableBBG": false,
  "AnyKernel3": {
    "use": true,
    "release": false,
    "custom": {
      "repo": "https://github.com/William24hmar/AnyKernel3.git",
      "branch": "master"
